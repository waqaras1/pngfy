{
  "name": "pngfy-pro",
  "version": "2.0.0",
  "description": "Professional background removal tool with modern UI/UX",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build:css && npm run build:js",
    "build:css": "sass src/styles/main.scss public/css/main.css --style compressed",
    "build:js": "webpack --mode production",
    "watch": "concurrently \"npm run watch:css\" \"npm run watch:js\"",
    "watch:css": "sass src/styles/main.scss public/css/main.css --watch",
    "watch:js": "webpack --mode development --watch"
  },
  "keywords": ["background-removal", "image-processing", "ai", "png", "web-app"],
  "author": "PngFy Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^2.0.0-rc.3",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "sass": "^1.69.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "concurrently": "^8.2.2",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "cssnano": "^6.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
} 