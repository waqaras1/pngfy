    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "sass": "^1.69.5",
    "concurrently": "^8.2.2"
  },
